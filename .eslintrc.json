{
  "extends": ["next/core-web-vitals", "plugin:jsx-a11y/recommended"],
  "plugins": ["jsx-a11y"],

  "rules": {
    "react/jsx-no-target-blank": [
      "error",
      {
        "allowReferrer": true,
        "enforceDynamicLinks": "always",
        "warnOnSpreadAttributes": false,
        "ignoreAttribute": ["netlify"]
      }
    ]
  }
}
